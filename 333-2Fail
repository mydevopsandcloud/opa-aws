resource "aws_networkfirewall_rule_group" "fail_case_empty_rules" {
  name        = "stateless-fail-empty-rules"
  description = "Stateless rule group with empty stateless_rule list"
  capacity    = 100
  type        = "STATELESS"

  rule_group {
    rules_source {
      stateless_rules_and_custom_actions {
        stateless_rule = []
      }
    }
  }

  tags = {
    Env = "TestFailEmptyRules"
  }
}
