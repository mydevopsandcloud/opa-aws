resource "aws_networkfirewall_rule_group" "example_fail" {
  description = "Stateful Rule Example"
  capacity    = 100
  name        = "example_fail"
  type        = "STATEFUL"  # This should fail the Rego rule
  
  rule_group {
    rules_source {
      stateful_rules {
        rule_definition {
          actions = ["aws:pass"]
          match_attributes {
            destination {
              address_definition = "1.1.1.1/32"
            }
          }
        }
      }
    }
  }
}
